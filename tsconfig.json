{
   "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "strictPropertyInitialization": false,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true
   }
}